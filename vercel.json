{
    "version": 2,
    "framework": null,
    "buildCommand": "npm run vercel-build",
    "installCommand": "composer install --no-dev --optimize-autoloader && npm install",
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/build/assets/(.*)",
            "dest": "/build/assets/$1",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/build/manifest.json",
            "dest": "/build/manifest.json",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/favicon.ico",
            "dest": "/favicon.ico"
        },
        {
            "src": "/favicon.svg",
            "dest": "/favicon.svg"
        },
        {
            "src": "/apple-touch-icon.png",
            "dest": "/apple-touch-icon.png"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://ikate.vercel.app",
        "ASSET_URL": "https://ikate.vercel.app",
        "APP_KEY": "base64:z7Po3je6m/QKy4UUAJhTWaxSKYYD0xC/QUeGEJYLjgM=",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}
